<%= form_for [@company, @job] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :level_of_interest %>
  <%= f.number_field :level_of_interest %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :category %>
  <%= f.select(:category, options_for_select([['NEW', 'NEW'], ['OLD', 'OLD']])) %>

  <%= f.submit %>
<% end %>
