<h2>All Jobs for <strong><%= @company.name %></strong></h2>

<% @jobs.each do |job| %>
  <ul><%= link_to "#{job.title} at #{@company.name}", company_job_path(@company, job) %></ul>
<% end %>

<%= link_to 'Create a New Job', new_company_job_path(@company) %><br/>
<%= link_to '<< Back to Company List', companies_path %>
